{
  "name": "doucement",
  "private": true,
  "version": "1.16.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "npm run build && playwright test",
    "test:e2e:ui": "npm run build && playwright test --ui",
    "test:e2e:head": "npm run build && playwright test --headed",
    "auto-implement": "./scripts/auto-implement.sh",
    "auto-implement:e2e": "./scripts/auto-implement.sh 10 true",
    "auto-implement:push": "./scripts/auto-implement.sh 10 true true"
  },
  "dependencies": {
    "emoji-picker-react": "^4.16.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.52.0",
    "vite": "^6.0.7",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16"
  }
}
